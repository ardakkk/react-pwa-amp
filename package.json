{
  "name": "pwa-amp-react",
  "version": "1.0.0",
  "description": "React with AMP and PWA",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --mode development --hot",
    "dev": "webpack-dev-server --open --mode development --hot",
    "transpile": "babel src -d dist --copy-files",
    "prepublishOnly": "npm run transpile",
    "build": "rm -rf dist; webpack --config ./webpack.prod.js --mode production ",
    "deploy": "deploy-to-git",
    "publish-demo": "npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ardakkk/react-pwa-amp/"
  },
  "author": "Ardak Tileu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ardakkk/react-pwa-amp/issues"
  },
  "homepage": "https://github.com/ardakkk/react-pwa-amp#readme",
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "autoprefixer": "^9.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "deploy-to-git": "^0.1.5",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.1",
    "file-loader": "^1.1.11",
    "generate-json-file-webpack-plugin": "0.0.3",
    "gh-pages": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-manifest-plugin": "^2.0.2"
  },
  "config": {
    "deployToGit": {
      "repository": "git@github.com:owner/your-repo.git",
      "branch": "dev",
      "folder": "build",
      "script": "npm run build",
      "commit": "Automatic commit text",
      "user": {
        "email": "your email",
        "name": "your name"
      }
    }
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "react-hot-loader": "^4.1.3"
  }
}

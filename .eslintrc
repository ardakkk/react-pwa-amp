{
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
      "semi": [1, "never"],
      "max-len": [1, 80],
      "arrow-parens": [2, "as-needed"],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [".js"]
        }
      ],
      "react/jsx-indent": [true, 4]
    }
  }